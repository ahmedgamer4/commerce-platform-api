{
  "name": "my-app",
  "version": "1.0.0",
  "description": "My app",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon ./src/server.ts",
    "clean": "rimraf build/",
    "build": "yarn clean && npx tsc",
    "start": "npm run build && nodemon build/server.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "migration:create": "db-migrate create test-table --sql-file",
    "migration:up": "db-migrate --env dev up",
    "migration:down": "db-migrate --env dev down",
    "migration:reset": "db-migrate --env dev reset",
    "test": "db-migrate --env test up && npx tsc && cross-env NODE_ENV=test jasmine && db-migrate --env test reset"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Mohamed Yasser",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Mohamed475/Image-processing-api/issues"
  },
  "homepage": "https://github.com/Mohamed475/Image-processing-api#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "express-rate-limit": "^6.6.0",
    "helmet": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.8.0",
    "uuid": "^9.0.0",
    "validator": "^13.7.0"
  },
  "keywords": [],
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.11.12",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.7",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.2",
    "yarn": "^1.22.19"
  }
}
